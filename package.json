{
  "name": "treksoul",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cp .env.environment-localhost .env.development && npm i && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev_replit": "cp .env.environment-replit .env.development && npm i && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
  },
  "dependencies": {
    "@payloadcms/db-mongodb": "3.40.0",
    "@payloadcms/live-preview-react": "3.40.0",
    "@payloadcms/next": "3.40.0",
    "@payloadcms/payload-cloud": "3.40.0",
    "@payloadcms/richtext-lexical": "3.40.0",
    "@payloadcms/storage-s3": "3.40.0",
    "@payloadcms/ui": "3.40.0",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/postcss": "^4.1.8",
    "axios": "^1.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cross-env": "^7.0.3",
    "d3-format": "^3.1.0",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.17.3",
    "graphql": "^16.8.1",
    "lucide-react": "^0.511.0",
    "next": "^15.3.3",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.3",
    "payload": "3.40.0",
    "postcss": "^8.5.4",
    "react": "^19.1.0",
    "react-dom": "19.1.0",
    "react-icons": "^5.5.0",
    "sharp": "0.32.6",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@types/axios": "^0.14.4",
    "@types/d3-format": "^3.0.4",
    "@types/node": "^22.5.4",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.2",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.3.0",
    "prettier": "^3.4.2",
    "tw-animate-css": "^1.3.2",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  }
}
